{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Air",
      "type": "shell",
      "command": "air",
      "options": {
        "cwd": "${workspaceFolder}/task4"
      },
      "isBackground": true,
      "problemMatcher": {
        "owner": "custom",
        "pattern": { "regexp": "." },
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".",
          "endsPattern": "tmp/main.exe"
        }
      }
    }
  ]
}